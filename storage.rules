
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow read/write access to all files for now
    // This is for development and testing ONLY.
    // Lock this down before production.
    match /{allPaths=**} {
      allow read, write: if request.time < timestamp.date(2024, 8, 30);
    }
  }
}
