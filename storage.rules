
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      // Allow read and write access for all users, including unauthenticated ones.
      // This is insecure and should only be used for a short time during setup or for a demo.
      allow read, write: if true;
    }
  }
}
