
rules_version = '2';

// CAUTION: These rules are for development testing only.
// They allow public read and write access to all files in your Storage bucket.
// Before deploying to production, you must secure these rules.
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
