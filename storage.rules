rules_version = '2';

// WARNING: These rules are insecure and grant public read/write access.
// They are intended for short-term development and demonstration ONLY.
// Re-secure your rules before launching to production.
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
