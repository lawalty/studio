
rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: Insecure. Allows anyone to read/write to your storage bucket.
    // This is for debugging and initial development ONLY.
    // Secure your data before going to production.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
