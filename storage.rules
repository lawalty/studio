rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // This rule allows anyone to upload, view, and delete files in your storage
    // bucket. It will expire in 30 days.
    // WARNING: This is not a secure rule for production.
    match /{allPaths=**} {
      allow read, write: if request.time < timestamp.now() + duration.time(30, 0, 0, 0);
    }
  }
}
