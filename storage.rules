rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: This is an insecure rule for debugging purposes.
    // It allows any user, authenticated or not, to read, write, and delete
    // any file in your storage bucket.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
