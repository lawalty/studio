
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // This rule grants access to all files in your storage for the next 30 days.
    // After that, all requests will be denied.
    match /{allPaths=**} {
      allow read, write: if request.time < timestamp.date(2024, 8, 14);
    }
  }
}
