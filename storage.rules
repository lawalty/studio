rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow read and write access for all paths to any user.
    // This is a "test mode" rule for development and should be secured
    // before going to production.
    match /{allPaths=**} {
      allow read, write: if request.time < timestamp.date(2024, 8, 22);
    }
  }
}
