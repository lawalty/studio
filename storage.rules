rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow read and write access for all paths to any authenticated user.
    // This is a common setup for apps where users manage their own content.
    // For more granular control, you can add path-specific rules.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
