
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: Insecure rules. Allow all read/write access for 30 days for development.
    // This should be secured before going to a real production environment.
    match /{allPaths=**} {
      allow read, write: if request.time < timestamp.date(2024, 8, 15);
    }
  }
}
