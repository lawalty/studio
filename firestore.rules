rules_version = '2';

// Set to "test mode" for 30 days to allow all reads and writes.
// This is for development and demonstration purposes.
// TODO: Secure these rules before a full production launch.
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2024, 8, 17);
    }
  }
}
