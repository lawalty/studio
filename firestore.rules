
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: Insecure. Allows read/write access to your entire database.
    // This is for debugging and initial development ONLY.
    // Secure your data before going to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
