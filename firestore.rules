rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These are temporary, insecure rules for development and demonstration.
    // They allow anyone to read and write to your entire database.
    // These should be secured before launching to a real audience.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
