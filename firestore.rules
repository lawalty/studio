
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // This rule grants access to all documents in your database for the next 30 days.
    // After that, all requests will be denied.
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2024, 8, 14);
    }
  }
}
